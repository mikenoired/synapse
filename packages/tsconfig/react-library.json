{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "../types/*.d.ts"
  ],
  "exclude": [
    "dist",
    "build",
    "node_modules"
  ]
}